/* components/w-tab-control/w-tab-control.wxss */
.tab-control {
  display: flex;
  height: 88rpx;
  line-height: 88rpx;
  padding-bottom: 20rpx;
  background-color: #fff;
  /* background-color: orange; */
}

.tab-item {
  flex: 1;
  text-align: center;
}

.active {
  color: red;
}

.active text {
  padding: 20rpx 10rpx;
  border-bottom: 6rpx solid red;
}